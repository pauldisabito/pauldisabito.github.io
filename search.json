---
layout: none
---
[
  {% for page in site.pages %}
   {
     {% if page.title != nil %}
        "title"    : "{{ page.title | escape }}",
        "category" : "{{ page.category }}",
        "tags"     : "{{ page.tags | join: ', ' }}",
        "url"      : "{{ site.baseurl }}{{ page.url }}",
        "date"     : "{{ page.date }}",
        "content"  : "{{ page.content | strip_html | normalize_whitespace | strip_html | escape | normalize_whitespace }}"
     {% endif %}
   } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
